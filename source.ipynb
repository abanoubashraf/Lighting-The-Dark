def saveImage(Img, imagName):
  inRGBImage = Img.astype(np.uint8)
  inRGBImage = Image.fromarray(inRGBImage, 'RGB')
  b, g, r = inRGBImage.split()
  inRGBImage = Image.merge("RGB", (r, g, b))
  inRGBImage.save(imagName)
  
  
